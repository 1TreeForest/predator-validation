{% if not cookieBarHide %}
  <div id="cookie-bar" class="alert alert-default">
    <div class="container">
      {{ 'msg.CookiesWarning'|trans|raw }}
      <a href="#" id="cookieBarAgree" data-role="cookie-bar-button" data-action="agree" class="btn btn-xs btn-success">{{ 'lbl.IAgree'|trans|ucfirst }}</a>
      <a href="#" id="cookieBarDisagree" data-role="cookie-bar-button" data-action="disagree" class="btn btn-xs btn-danger">{{ 'lbl.IDisagree'|trans|ucfirst }}</a>
    </div>
  </div>
{% endif %}
